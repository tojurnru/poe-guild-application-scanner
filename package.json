{
  "name": "poe-guild-application-scanner",
  "version": "0.0.1",
  "description": "Scan our guild application to get their poe profile and check if they are blacklisted in TFT",
  "license": "Apache-2.0",
  "volta": {
    "node": "16.8.0",
    "yarn": "1.22.15"
  },
  "scripts": {
    "dev": "ts-node -r dotenv/config src/processor.ts",
    "start": "node build/src/processor.js",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.10",
    "csv-parse": "^4.16.3",
    "discord.js": "^13.1.0",
    "moment": "^2.29.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "dotenv": "^10.0.0",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "~2.3.2",
    "ts-node": "^10.2.1",
    "tslib": "~2.3.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  }
}